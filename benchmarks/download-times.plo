#proc getdata
    file: @TABLEFILE

#proc areadef
    rectangle: 1 1 4 4
    xrange: 0 120
    xaxis.stubs: inc
    xaxis.label: Number of peers
    xaxis.grid: color=0.8
    yrange: 0 400
    yaxis.grid: color=0.8
    yaxis.label: Download time (s)
    yaxis.stubs: inc

#proc scatterplot
    xfield: 1
    yfield: 2
    symbol: shape=circle color=red radius=0.04
